                            {

                                <tr>
                                    <td>@item.Isim</td>

                                    <td>@item.StokAdeti</td>

                                   >

                                </tr>
                            }
                        }
                        else
                        {
                            <p>STOK BULUNMAMAKTA</p>
                        }

                            </tbody>
                        </table>

                    </div>
                </div>
            </div>



            <div class="text-center">
                <ul class="pagination">
                </ul>
            </div><!-- /.text-center -->



        </div>




        </div><!-- /.container -->








    </body>




