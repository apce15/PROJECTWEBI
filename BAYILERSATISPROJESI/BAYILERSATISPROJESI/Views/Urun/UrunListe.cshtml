@using BAYILERSATISPROJESI.Models
@model List<urunSet>


<body>



    <div class="container container-int">



        <div class="row">
            <div class="col-lg-12">

                <hr />
                <div class="col-md-12">
                    <div class="alert alert-primary" role="alert">

                    </div>

                </div>

                <hr />



            </div>

            <div class="col-md-8 col-sm-8">
                <div class="col-md-9">




                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>ÜRÜN ID</th>
                                <th>ÜRÜN ADI</th>

                                <th>FİYAT</th>
                               
                            </tr>


                        </thead>

                        <tbody>


                            @if (Model != null)
                            {
                                foreach (var urn in Model)
                                {

                                    <tr>
                                        <td>@urn.Id </td>
                                        <td>@urn.Isim </td>

                                        <td>@urn.Fiyat </td>
                                        
                                    </tr>
                                }
                            }
                            else
                            {
                                <p>Ürün Bulunamadı.</p>
                            }

                        </tbody>
                    </table>
                   
                </div>
            </div>
        </div>



        <div class="text-center">
            <ul class="pagination">
            </ul>
        </div><!-- /.text-center -->



    </div>



    
    </div><!-- /.container -->








</body>




